{
    "name": "your-vendor/ksf-common",
    "description": "A common library for KSF projects.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ksfraser\\Common\\Models\\": "src/Models/",
            "Ksfraser\\Common\\Views\\": "src/Views/",
            "Ksfraser\\Common\\Controllers\\": "src/Controllers/",
            "Ksfraser\\Common\\Core\\": "src/Core/"
        }
    },
    "require": {
        "ksfraser/origin": "dev-main"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ksfraser/origin"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
