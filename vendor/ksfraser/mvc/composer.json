{
    "name": "ksfraser/X",
    "type": "library",
    "description": "Magic Library.  ***CHANGEME***    library",
    "keywords": [
        "framework",
    ],
    "homepage": "http://ksfraser.ca/",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Fraser",
            "email": "kevin@ksfraser.ca",
            "homepage": "https://ksfraser.ca/"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "psr/http-client": "^1.0"
    },
    "provide": {
        "psr/http-client-implementation": "1.0"
    },
    "require-dev": {
        "psr/log": "^1.1"
    },
    "suggest": {
        "psr/log": "Required for using the Log middleware"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "GuzzleHttp\\": "src/"
        },
        "files": [
            "src/functions_include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GuzzleHttp\\Tests\\": "tests/"
        }
    }
}
